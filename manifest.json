{
    "name": "NetSuite Help Center Actual Page Title",
    "description": "Puts the title of what you have opened in the actual tab title, and changes the tab icon, in the NS Help Center.",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://*.netsuite.com/app/help/helpcenter.nl?*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/images/myfavicon.png"
            ],
            "matches": [
                "https://*.app.netsuite.com/*"
              ]
        }
    ],
    "icons": {
        "48": "/images/myicon.png",
        "128": "/images/myicon.png"
    }
}